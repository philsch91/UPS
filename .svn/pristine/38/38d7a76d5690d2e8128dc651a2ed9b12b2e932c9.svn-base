<Window x:Class="UPS.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Login" Height="220" Width="280" WindowStartupLocation="CenterOwner" ResizeMode="NoResize">
    <Grid FocusManager.FocusedElement="{Binding ElementName=passwordBox}">
        <TextBox Height="23" HorizontalAlignment="Left" Margin="81,47,0,0" Name="textBoxUser" VerticalAlignment="Top" Width="120" />
        <Label Content="Username" Height="28" HorizontalAlignment="Left" Margin="15,47,0,0" Name="labelUser" VerticalAlignment="Top" />
        <Label Content="Password" Height="28" HorizontalAlignment="Left" Margin="18,74,0,0" Name="labelPassword" VerticalAlignment="Top" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="81,74,0,0" Name="passwordBox" VerticalAlignment="Top" Width="120" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="81,103,0,0" Name="buttonOK" VerticalAlignment="Top" Width="75" Click="buttonOK_Click" IsDefault="True" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="18,18,0,0" Name="textBlockStatus" Text="" VerticalAlignment="Top" Width="228" />
        <CheckBox Content="Debug" Height="16" HorizontalAlignment="Left" Margin="193,153,0,0" Name="debugCheckBox" VerticalAlignment="Top" />
    </Grid>
</Window>
